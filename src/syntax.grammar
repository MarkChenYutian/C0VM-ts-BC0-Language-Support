@top Program { MagicNumber expression* }

@skip { space }

expression {
  Byte |
  FuncHeader |
  JMPLabel | 
  LineComment
}

@tokens {
  MagicNumber { "C0 C0 FF EE" }
  Byte { $[A-Fa-f0-9]$[A-Fa-f0-9] }
  FuncHeader { "#<" $[A-Za-z_\-0-9]+ ">" }
  JMPLabel { "# <" $[A-Fa-f0-9]+ ":" $[A-Za-z_\-0-9] ">" }
  LineComment { "# " ![<\n]![\n]* }
  space { $[ \t\n\r]+ }
}

@detectDelim
